{
  "name": "trisog-api",
  "version": "1.0.0",
  "description": "Trisog API is a RESTful API that powers the backend for a travel and tourism platform, providing endpoints for managing destinations, experiences, user reviews, bookings, and more. Built with Node.js, Express.js, TypeScript, MongoDB, and Prisma, the API ensures robust and scalable data handling, secure user authentication, and efficient media storage.",
  "main": "index.js",
  "repository": "https://github.com/ViniciusGR797/trisog-api.git",
  "author": "ViniciusGR797 <loritoamarelo@outlook.com>",
  "license": "MIT",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "postinstall": "prisma generate",
    "prisma": "npx prisma generate",
    "swagger": "npx ts-node src/swagger/generateSwagger.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "axios": "^1.7.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.3.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.8.0",
    "prisma": "^5.17.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2"
  }
}
